{
    "name": "portphp/elasticsearch",
    "description": "Elasticsearch reader and writer for PortPHP",
    "license": "MIT",
    "homepage": "https://github.com/portphp",
    "authors": [
        {
            "name": "David de Boer",
            "email": "david@ddeboer.nl"
        },
        {
            "name": "Markus Bachmann",
            "email": "markus.bachmann@bachi.biz"
        },
        {
            "name": "Community contributors",
            "homepage": "https://github.com/portphp/elasticsearch/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/portphp/portphp/issues",
        "source": "https://github.com/portphp/elasticsearch",
        "docs": "http://docs.portphp.org"
    },
    "require": {
        "php": ">=7.0",
        "elasticsearch/elasticsearch": "^6.0",
        "portphp/portphp": "^1.0"
        },
    "autoload": {
        "psr-4": {
            "Port\\Elasticsearch\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0",
        "phpspec/phpspec": "^2.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Port\\Elasticsearch\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
